# Git stuff
alias g="git"
alias gcm="git checkout master"
alias gdm="git branch --merged | egrep -v '(^\*|master|dev)' | xargs git branch -d"
alias git-delete-merged="gdm"

# Edit this aliases file in sublime
alias subl-al="subl ~/.aliases"

# Source this aliases file
alias source-al="source ~/.aliases"

# Unsecure Chrome (Chrome with disable security flag)
alias uc="open -n /Applications/Google\ Chrome.app --args --disable-web-security"
# Unsecure Safari (Safari with disable security flag)
alias us="open -a '/Applications/Safari.app' --args --disable-web-security"

# Because sometimes my mac stops seeing my camera
alias camreset="sudo killall VDCAssistant && sudo killall AppleCameraAssistant"
